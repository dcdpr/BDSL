{
  "canvas": {
    "place": {
      "$description": "A group of affordances on the breadboard.",
      "header": {
        "$description": "The collection of place title, number, and underline.",
        "padding-bottom": {
          "$type": "number",
          "$value": 20.0
        },
        "title": {
          "$description": "The title of the place.",
          "font-size": {
            "$type": "number",
            "$value": 20.0
          },
          "padding-bottom": {
            "$type": "number",
            "$value": 2.0
          },
          "number": {
            "$description": "The number in front of the title.",
            "font-size": {
              "$type": "number",
              "$value": 15.0
            }
          }
        }
      }
    },
    "affordance": {
      "$description": "A single affordance in a place.",
      "font": {
        "$value": "{_fonts._shantell-sans}"
      },
      "padding_bottom": {
        "$type": "number",
        "$value": 4.0
      },
      "level_padding": {
        "$type": "number",
        "$value": 10.0
      }
    }
  },
  "egui": {
    "$description": "Egui-specific styling for the non-canvas UI elements.",
    "visuals": {
      "override_text_color": {
        "$value": "{colors.text}"
      },
      "hyperlink_color": {
        "$value": "{colors._rosewater}"
      },
      "faint_bg_color": {
        "$value": "{colors._surface0}"
      },
      "extreme_bg_color": {
        "$value": "{colors._crust}"
      },
      "code_bg_color": {
        "$value": "{colors._mantle}"
      },
      "warn_fg_color": {
        "$value": "{colors._peach}"
      },
      "error_fg_color": {
        "$value": "{colors._maroon}"
      },
      "window_fill": {
        "$value": "{colors._base}"
      },
      "panel_fill": {
        "$value": "{colors._base}"
      },
      "window_stroke": {
        "$value": "{colors.overlay1}"
      },
      "widgets": {
        "noninteractive": {
          "$value": "{colors._base}"
        },
        "inactive": {
          "$value": "{colors._surface0}"
        },
        "hovered": {
          "$value": "{colors._surface2}"
        },
        "active": {
          "$value": "{colors._surface1}"
        },
        "open": {
          "$value": "{colors._surface0}"
        }
      },
      "selection": {
        "$value": "{colors._blue}"
      },
      "window_shadow": {
        "$value": "{colors._base}"
      },
      "popup_shadow": {
        "$value": "{colors._base}"
      }
    }
  },
  "_fonts": {
    "$type": "fontFamily",
    "_shantell-sans": {
      "$value": [
        "ShantellSans-Regular"
      ]
    },
    "_permanent-marker": {
      "$value": [
        "PermanentMarker-Regular"
      ]
    }
  },
  "colors": {
    "$type": "color",
    "_rosewater": {
      "$value": "#f2d5cf"
    },
    "_flamingo": {
      "$value": "#eebebe"
    },
    "_pink": {
      "$value": "#f4b8e4"
    },
    "_mauve": {
      "$value": "#ca9ee6"
    },
    "_red": {
      "$value": "#e78284"
    },
    "_maroon": {
      "$value": "#ea999c"
    },
    "_peach": {
      "$value": "#ef9f76"
    },
    "_yellow": {
      "$value": "#e5c890"
    },
    "_green": {
      "$value": "#a6d189"
    },
    "_teal": {
      "$value": "#81c8be"
    },
    "_sky": {
      "$value": "#99d1db"
    },
    "_sapphire": {
      "$value": "#85c1dc"
    },
    "_blue": {
      "$value": "#8caaee"
    },
    "_lavender": {
      "$value": "#babbf1"
    },
    "text": {
      "$value": "#c6d0f5"
    },
    "_subtext1": {
      "$value": "#b5bfe2"
    },
    "_subtext0": {
      "$value": "#a5adce"
